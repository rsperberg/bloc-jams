{
   "globals": {
      "$": true,
      "$()": true,
      "document": true,
      "window": true
    },
   "rules": {
      "quotes": [2, "single", "avoid-escape"],
      "func-names": 0,
      "func-style": [0, "declaration"],
      "curly": [1, "all"],
      "no-unused-vars": [2, {"vars": "local", "args": "none"}]
    }
}
